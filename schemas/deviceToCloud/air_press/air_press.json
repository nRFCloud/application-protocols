{
    "title":"Air pressure",
    "description":"The device's air pressure sensor in pascals (Pa)",
    "type":"object",
    "properties":{
       "appId":{
          "type":"string",
          "const":"AIR_PRESS"
       },
       "messageType":{
          "type":"string",
          "const":"DATA"
       },
       "data":{
          "type":"string",
          "pattern":"^([0-9]|[1-9][0-9]+)\\.[0-9]+$"
       }
    },
    "required":[
       "appId",
       "messageType",
       "data"
    ],
    "additionalProperties":false
 }
