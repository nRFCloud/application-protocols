{
    "title": "Agps",
    "description": "AGPS request",
    "type": "object",
    "properties": {
        "appId": {
            "type": "string",
            "const": "AGPS"
        },
        "messageType": {
            "type": "string",
            "const": "DATA"
        },
        "data": {
            "type": "object",
            "properties": {
                "mcc": {
                    "$ref": "#/definitions/Mcc"
                },
                "mnc": {
                    "$ref": "#/definitions/Mnc"
                },
                "tac": {
                    "$ref": "#/definitions/Tac"
                },
                "eci": {
                    "$ref": "#/definitions/Eci"
                },
                "rsrp": {
                    "$ref": "#/definitions/Rsrp"
                },
                "types": {
                    "$ref": "#/definitions/Types"
                },
                "filtered": {
                    "$ref": "#/definitions/Filtered"
                },
                "mask": {
                    "$ref": "#/definitions/Mask"
                },
                "additionalProperties": false
            }
        }
    },
    "definitions": {
        "Mcc": {
            "type": "integer",
            "description": "Mobile Country Code"
        },
        "Mnc": {
            "type": "integer",
            "description": "Mobile Network Code"
        },
        "Tac": {
            "type": "integer",
            "description": "Tracking Area Code"
        },
        "Eci": {
            "type": "integer",
            "description": "E-UTRA Cell Identifier"
        },
        "Rsrp": {
            "type": "number",
            "description": "Signal power",
            "minimum": -157,
            "maximum": -44
        },
        "Types": {
            "type": "array",
            "description": "Message Type. 1 = utc parameters, 2 = ephemeries data, 3 = almanac data, 4 = klobuchar ionospheric correction, 5 = nequick ionospheric correction, 6 = gps time of week, 7 = gps system clock, 8 = location (lat/lon of cell tower) 9 = gps integrity info",
            "items": {
                "type": "integer",
                "enum": [1, 2, 3, 4, 5, 6, 7, 8, 9],
                "maximum": 9,
                "minimum": 1
            },
            "minItems": 1,
            "uniqueItems": true
        },
        "Filtered": {
            "type": "boolean",
            "description": "If true, returns only ephemeris for visible satellites. Requires cell tower info."
        },
        "Mask": {
            "type": "integer",
            "description": "Filtered ephemeris mask angle. Only works in conjunction with the filtered flag. Controls the mask angle for which the satellites are filtered. Defaults to 5.",
            "minimum": 0,
            "maximum": 90
        }
    },
    "required": [
    ],
    "additionalProperties": false
}