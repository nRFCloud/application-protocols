{
    "title": "Temperature",
    "description": "The device's temperature sensor reading in Celsius",
    "examples": [
        {"appId":"TEMP","messageType":"DATA","data":"24.3"}
    ],
    "type": "object",
    "properties": {
        "appId": {
            "type": "string",
            "const": "TEMP"
        },
        "messageType": {
            "type": "string",
            "const": "DATA"
        },
        "data": {
            "type": "string",
            "pattern": "^([0-9]|[1-9][0-9]+)\\.[0-9]+$"
        }
    },
    "additionalProperties": false,
    "required": [
        "appId",
        "messageType"
    ]
}
