{
    "compilerOptions": {
        "module": "es2015",
        "target": "es2015",
        "strict": true,
        "outDir": "dist/",
        "moduleResolution": "node",
        "declaration": true,
        "skipLibCheck": true,
        "esModuleInterop": true,
        "noUnusedParameters": true,
        "noUnusedLocals": true,
        "resolveJsonModule": true
    }
}
